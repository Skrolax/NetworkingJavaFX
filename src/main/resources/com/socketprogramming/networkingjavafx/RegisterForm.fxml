<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.socketprogramming.networkingjavafx.controllers.RegisterFormController">
   <children>
      <TextField fx:id="usernameRegisterField" layoutX="225.0" layoutY="114.0" promptText="Username" />
      <TextField fx:id="passwordRegisterField" layoutX="225.0" layoutY="161.0" promptText="Password" />
      <TextField fx:id="emailRegisterField" layoutX="225.0" layoutY="214.0" promptText="Email" />
      <Button fx:id="registerButton" layoutX="270.0" layoutY="271.0" mnemonicParsing="false" onAction="#registerUser" text="Register" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="224.0" layoutY="59.0" prefHeight="18.0" prefWidth="150.0" text="Register Form" textAlignment="CENTER" />
      <Label fx:id="registerStatusLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="240.0" layoutY="310.0" prefHeight="18.0" prefWidth="0.0" textAlignment="CENTER" />
   </children>
</AnchorPane>
